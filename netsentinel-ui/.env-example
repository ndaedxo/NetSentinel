# Environment Variables for Netsentinel
# Copy this file to .env and configure your values

# Database Configuration (for Cloudflare D1)
# These are auto-configured when you deploy to Cloudflare
# DATABASE_URL=your-database-url

# Authentication (implement your own auth system)
# JWT_SECRET=your-jwt-secret-here
# API_KEY=your-api-key-here

# Optional: Development settings
NODE_ENV=development

# Sentry Error Monitoring (get DSN from https://sentry.io)
VITE_SENTRY_DSN=https://your-sentry-dsn-here@sentry.io/project-id
VITE_SENTRY_ENVIRONMENT=development
